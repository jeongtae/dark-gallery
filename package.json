{
  "name": "electron-svelte-template",
  "productName": "Dark Gallery",
  "version": "1.0.0",
  "description": "최고의 사진 및 비디오 분류 앱",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "run-p public:watch main:watch svelte:watch forge:start",
    "prestart": "run-s public:build main:dev",
    "package": "run-s clean public:build main:build svelte:build forge:package",
    "format": "prettier --write \"**/*.{js,ts,json,svelte,html,css,scss}\"",
    "clean": "rimraf dist out",
    "main:build": "gulp buildMain",
    "main:dev": "gulp devMain",
    "main:watch": "gulp watchMain",
    "public:build": "gulp buildPublic",
    "public:watch": "gulp watchPublic",
    "svelte:build": "rollup -c",
    "svelte:watch": "rollup -c -w",
    "forge:start": "electron-forge start",
    "forge:package": "electron-forge package",
    "forge:make": "electron-forge make",
    "forge:publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "Jeongtae Kim",
    "email": "rsatang5@naver.com"
  },
  "license": "GPLv3",
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/gulp": "^4.0.7",
    "@types/lodash": "^4.14.162",
    "@types/rimraf": "^3.0.0",
    "carbon-components-svelte": "^0.21.0",
    "carbon-icons-svelte": "^10.17.0",
    "electron": "^10.1.5",
    "electron-packager-languages": "^0.4.0",
    "electron-reload": "^1.5.0",
    "electron-typed-ipc": "^0.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-filter": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.1",
    "rollup": "^2.29.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.27.0",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.5.1",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.5",
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.20",
    "open-color": "^1.7.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sharp": "^0.26.3",
    "sqlite3": "^5.0.0"
  }
}
